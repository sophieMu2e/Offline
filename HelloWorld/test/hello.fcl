#include "fcl/minimalMessageService.fcl"

# Give this job a name.
process : HelloWorld

# Start form an empty source
source : {
  module_type : EmptyEvent
  maxEvents : 3
}

services : {
  message : @local::default_message
}

physics :{
  analyzers: {
    hello: {
      module_type : HelloWorld
    }
  }

  p1 : [ ]
  e1 : [hello]

  trigger_paths  : [p1]
  end_paths      : [e1]

}


